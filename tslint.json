{
  "rulesDirectory": "./node_modules/core-decorators",
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "experimentalDecorators": true

  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}